# XO Chain Bundler Configuration

# Chain
NETWORK=xo
NODE_HTTP=https://rpc-mainnet-2.xo.market

# Signer - replace with your actual private key
SIGNER_PRIVATE_KEYS=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# Unsafe mode - skips debug_traceCall simulation checks
UNSAFE=true

# Bundle send interval in milliseconds (lower = faster)
CHAIN_BUNDLE_MAX_SEND_INTERVAL_MILLIS=25

# Pool
POOL_CHAIN_POLL_INTERVAL_MILLIS=25

# Limit block range for getUserOperationReceipt lookups (RPC node max is 100k)
USER_OPERATION_EVENT_BLOCK_DISTANCE=50000

# RPC
RPC_API=eth,debug,rundler
RPC_HOST=0.0.0.0

# Logging
RUST_LOG=info

# Entry points
ENABLED_ENTRY_POINTS=v0.7
